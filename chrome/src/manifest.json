{
  "manifest_version": 2,
  "name": "Twipster",
  "description": "Simpler, Readabler Twitter",
  "icons": { 
    "48": "icon48.png",
    "128": "icon128.png" },
  "version": "2.0.1",
  "homepage_url": "http://github.com/jxnblk/Twipster",
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "css": ["twipster.css"],
      "run_at": "document_end"
    }
  ]
}
